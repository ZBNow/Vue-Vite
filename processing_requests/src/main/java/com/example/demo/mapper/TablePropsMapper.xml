<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.TablePropsMapper">
    <select id="getTableProps" parameterType="java.lang.String"
            resultType="com.example.demo.dao.TableProps">
        select * from tableprops where  tableName =#{tableName};
    </select>
</mapper>