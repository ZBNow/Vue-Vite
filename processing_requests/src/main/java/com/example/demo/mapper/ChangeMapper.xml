<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.ChangeMapper">
    <insert id="change" parameterType="com.example.demo.dao.Question">
        update questions set
            `LMC`=#{LMC},
            `title`=#{title}
        where `id`=#{id}
    </insert>
</mapper>
